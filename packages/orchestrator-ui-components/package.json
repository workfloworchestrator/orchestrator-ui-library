        "@testing-library/react": "^16.0.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/invariant": "^2.2.33",
        "@types/jest": "^29.5.8",
        "@types/object-hash": "^3.0.6",
        "@types/scroll-into-view": "^1.16.3",
        "babel-jest": "^29.7.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
