{
  "extends": "@orchestrator-ui/tsconfig/base.json",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "noEmit": true
  },
  "include": ["src", "./src/**/*.ts", "./src/**/*.tsx"],
  "exclude": ["node_modules", "**/*.stories.ts", "**/*.stories.js", "**/*.stories.jsx", "**/*.stories.tsx"]
}
