{
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "importOrder": [
        "^react", "^react-native", "^[a-zA-Z]", "^@?\\w", "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ]
}
