{
    "private": true,
    "scripts": {
        "dev": "turbo run dev --filter wfo-ui",
        "dev:all": "turbo run dev",
        "tsc": "turbo run tsc",
        "lint": "turbo run lint",
        "prettier": "prettier -c \"{apps/**/*.{ts,tsx,json,md},packages/**/*{ts,tsx,json,md},*.{ts,js,json,md}}\"",
        "prettier-fix": "prettier --write \"{apps/**/*.{ts,tsx,json,md},packages/**/*{ts,tsx,json,md},*.{ts,js,json,md}}\"",
        "build": "turbo run build",
        "publish-packages": "turbo run build lint test && changeset version && changeset publish"
    },
    "devDependencies": {
        "@changesets/cli": "^2.26.2",
        "@turbo/gen": "^1.9.7",
        "eslint": "^7.32.0",
        "eslint-config-custom": "*",
        "prettier": "^2.5.1",
        "turbo": "^1.10.9"
    },
    "name": "orchestrator-core-ui",
    "packageManager": "yarn@1.22.19",
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
}
